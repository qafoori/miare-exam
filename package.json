{
  "name": "miare-exam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "next lint",
    "browserslist": "npx browserslist",
    "prepare": "husky install",
    "format": "npx prettier --write .",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "cy:chrome": "cypress run --browser chrome --reporter json",
    "cy:firefox": "cypress run --browser firefox --reporter json",
    "cy:edge": "cypress run --browser edge --reporter json",
    "cy:clear-reps:wins": "del .\\reporters\\reports\\junit\\*",
    "cy:clear-reps:unix": "rm -r reporters/reports/junit/*",
    "jest:run": "jest --silent=true",
    "jest:watch": "jest --watch --silent=false",
    "jest:ci": "jest --ci"
  },
  "dependencies": {
    "antd": "^4.18.9",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "redux": "^4.2.0",
    "sass": "^1.51.0",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-helmet": "^6.1.5",
    "@types/styled-components": "^5.1.25",
    "cypress": "^9.6.0",
    "cypress-multi-reporters": "^1.6.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
